<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.sodepebrasil.sodepeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:theme="@style/AppTheme.Toobar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login" android:theme="@style/AppTheme.Toobar"/>
        <activity android:name=".ConfiguracoesActivity" android:theme="@style/AppTheme.Toobar"/>
        <activity android:name=".Funcao1Activity" android:theme="@style/AppTheme.Toobar"/>
        <activity android:name=".Funcao2Activity" android:theme="@style/AppTheme.Toobar"/>
        <activity android:name=".Funcao3Activity" android:theme="@style/AppTheme.Toobar"/>
        <activity android:name=".SobreActivity" android:theme="@style/AppTheme.Toobar"/>
        <activity android:name=".CadastroActivity" android:theme="@style/AppTheme.Toobar"/>
    </application>

</manifest>